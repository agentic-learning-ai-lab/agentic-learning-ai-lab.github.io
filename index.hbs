<!doctype html>
<html lang="en">

<head>
    <!-- Open Graph / Facebook -->
    <meta property="og:title" content="Home | Agentic Learning AI Lab" />
    <meta property="og:description"
        content="Agentic Learning AI Lab is a research lab in New York University founded in 2022. We innovate learning algorithms that enable future agentic AI to learn and adapt flexibly in the real world." />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://agenticlearning.ai/" />
    <!--Replace with the current website url-->
    <meta property="og:image" content="https://agenticlearning.ai/assets/images/background/bg3.png" />
    {{> head}}
</head>

<body class="tw-flex tw-min-h-[100vh] tw-flex-col tw-bg-[#fff] tw-font-mono">
    {{> header links }}

    <section
        class="tw-relative tw-flex tw-w-full tw-max-w-[100vw] tw-min-w-[350px] tw-flex-col tw-overflow-hidden tw-mt-[150px] tw-px-[5%] max-md:tw-px-4 max-lg:tw-px-4 lg:tw-justify-around xl:tw-justify-around">
        <div class="tw-flex tw-w-full tw-place-content-center tw-gap-6 max-xl:tw-place-items-center max-lg:tw-flex-col">
            <div class="image-container">
                <img src="/assets/images/background/bg3.png" alt="app"
                    class="reveal-hero-img tw-z-[1] tw-h-full tw-w-full tw-object-contain" />

                <div class="text-overlay">
                    <div class="tw-flex tw-flex-wrap tw-font-semibold tw-leading-[85px] tw-text-7xl max-md:tw-text-4xl max-md:tw-leading-snug"
                        style={position: relative}>
                        <span class="reveal-hero-text">
                            agentic learning <br /> ai lab
                        </span>
                    </div>
                    <div
                        class="reveal-hero-text tw-mt-5 tw-max-w-[600px] tw-p-2 tw-text-justify max-lg:min(tw-max-w-full,tw-max-w-[600px])">
                        Agentic Learning AI Lab is a research lab in New York
                        University founded in 2022. We innovate learning algorithms that enable future agentic AI to
                        learn and adapt flexibly in the real world.
                    </div>
                </div>
            </div>
            <!-- <div class="tw-flex tw-flex-col tw-place-content-center"> -->
            <!-- </div> -->
        </div>
    </section>

    <section class="tw-mt-5 tw-flex tw-w-full tw-flex-col tw-p-[5%] max-lg:tw-p-4">
        <!-- reveal-up -->
        <h3 class="text-left tw-text-4xl tw-font-medium max-md:tw-text-2xl">
            Key Areas
        </h3>
        <!-- reveal-up -->
        <div class="section-underline tw-my-4">
        </div>

        <!-- reveal-up -->
        <div class="tw-mt-4 tw-space-y-8 tw-gap-10 sm:tw-columns-1 md:tw-columns-3 lg:tw-columns-3 xl:tw-columns-3">
            <!-- tw-place-content-center tw-justify-around -->
            {{> research_area_tabs research_areas }}
        </div>
    </section>

    <section class="tw-mt-4 tw-flex tw-w-full tw-flex-col tw-px-[5%] max-lg:tw-p-4 tw-overflow-hidden">
        <!-- reveal-up -->
        <h3 class="text-left tw-text-4xl tw-font-medium max-md:tw-text-2xl">
            Recent Works
        </h3>

        <!-- reveal-up -->
        <div class="section-underline tw-my-4">
        </div>
        <!-- tw-shadow-lg  -->
        <div
            class="tw-mt-4 tw-gap-10 tw-space-y-reverse sm:tw-columns-1 md:tw-columns-2 lg:tw-columns-3 xl:tw-columns-4 tw-items-start">
            {{> recent_works recent_papers }}
        </div>
    </section>

    {{> footer}}
</body>
<script src="/index.js"></script>

</html>